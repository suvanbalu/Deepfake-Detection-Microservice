{
  "efficientnet_variant": "b0",
  "data_dir": "faces",
  "data_augmentation":{
    "flag": true,
    "ratio": 0.5
  },
  "real_limit": -1,
  "fake_limit_scale": 1.5,
  "partition": {
    "flag": false,
    "n": 1,
    "p": 1,
    "real_limit_scale": -1,
    "fake_to_real_ratio":1,
    "format":"jpg"
  },
  "val_split": 0.2,
  "model": {
    "epochs": 30,
    "batch_size": 16,
    "learning_rate": 0.001,
    "decay": 0.9,
    "optimizer": "adam",
    "loss": "binary_crossentropy",
    "metrics": ["accuracy"],
    "output_path": "./faces_trained_model.h5",
    "regularization":{
      "flag":false,
      "type":"l2",
      "l2_lambda":0.01
    }
  },
  "plot": {
    "flag": true,
    "output_dir": "plots",
    "new_folder_flag": true,
    "types": ["loss", "accuracy"],
    "title": "Incremental Training Regularization Results"
  }
}
